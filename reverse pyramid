#include <stdio.h>

int main() {

  int size = 5;
  // loop to print the pattern
  for (int i = 0; i < size; i++) {
    // print spaces
    for (int j = 1; j < size-i; j++) {
      printf(" ");
    }
    // print number
    for (int k = 0; k <= i; k++) {
      printf("%d", (k+1));
    }
    printf("\n");
  }
  return 0;
}
